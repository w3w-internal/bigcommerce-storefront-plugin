  "devDependencies": {
    "@types/chance": "^1.1.3",
    "@types/jest": "^29.0.3",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "chance": "^1.1.8",
    "concurrently": "^7.6.0",
    "css-loader": "^6.7.2",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
